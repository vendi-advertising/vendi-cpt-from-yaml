{
  "name": "vendi-advertising/vendi-cpt-from-yaml",
  "license": "MIT",
  "description": "Loads CPTs from a yaml file",
  "authors": [
    {
      "name": "Chris Haas",
      "email": "chris@vendiadvertising.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "symfony/yaml": "^4.3|^5|^6",
    "webmozart/path-util": "^2.3",
    "vendi-advertising/vendi-yaml-loader": "^1.0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7|^8|^9",
    "sensiolabs/security-checker": "^6",
    "symfony/var-dumper": "^4|^5|^6",
    "mikey179/vfsstream": "^1.6"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Vendi\\CptFromYaml\\": "src/"
    }
  }
}
