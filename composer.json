{
  "name": "vendi-advertising/vendi-cpt-from-yaml",
  "license": "MIT",
  "description": "Loads CPTs from a yaml file",
  "authors": [
    {
      "name": "Chris Haas",
      "email": "chris@vendiadvertising.com"
    }
  ],
  "require": {
    "php": "8.0.*|8.1.*|8.2.*",
    "symfony/yaml": "^5|^6",
    "vendi-advertising/vendi-yaml-loader": "^2|^3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^8|^9",
    "mikey179/vfsstream": "^1.6"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Vendi\\CptFromYaml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vendi\\CptFromYaml\\tests\\": "tests/"
    }
  }
}
